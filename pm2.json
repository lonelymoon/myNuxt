{
  "apps": [
    {
      "name": "myNuxt",
      "script": "build/main.js",
      "watch": [
        "build",
        ".nuxt/dist"
      ],
      "watch_options": {
        "usePolling": true
      },
      "env": {
        "NODE_ENV": "development"
      },
      "env_production": {
        "NODE_ENV": "production"
      }
    }
  ]
}